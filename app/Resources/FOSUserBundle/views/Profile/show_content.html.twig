{% trans_default_domain 'FOSUserBundle' %}

<div class="fos_user_user_show">
    <p>{{ 'profile.show.username'|trans }}: {{ user.username }}</p>
    <p>{{ 'profile.show.email'|trans }}: {{ user.email }}</p>
    <a href="{{ path('fos_user_profile_edit') }}" class="btn btn-primary btn-lg">Modifier Profile</a>
    <a href="{{ path('fos_user_change_password') }}" class="btn btn-primary btn-lg">Changer Mots de Passe</a>

</div>
